obj-$(CONFIG_USB_BAM) += usb_bam.o
obj-$(CONFIG_I2C_MSM_PROF_DBG) += i2c-msm-prof-dbg.o
obj-$(CONFIG_IPA) += ipa/
obj-$(CONFIG_SPS) += sps/
obj-$(CONFIG_QPNP_POWER_ON) += qpnp-power-on.o
